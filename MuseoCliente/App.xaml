<Application x:Class="MuseoCliente.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="Login.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Custom Styles.xaml"/>
                <ResourceDictionary Source="/Designer/Views/FormStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <Style x:Key="IconosSocial">
                <Setter Property="TextElement.FontFamily" 
				Value="/MuseoCliente;component/Font/#Entypo-Social" />
            </Style>
            <Style x:Key="SourceSans">
                <Setter Property="TextElement.FontFamily" 
				Value="/MuseoCliente;component/Font/#Source Sans Pro"/>
            </Style>
        </ResourceDictionary>
    </Application.Resources>
</Application>
